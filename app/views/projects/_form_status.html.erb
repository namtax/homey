<div>
  <%= form_with model: @project do |form| %>
    <p>
      <%= form.select :status, [['In Progress', 'in_progress'],['Completed', 'completed']]  %>
    </p>
    <%= form.submit class: 'mt-4 px-6 py-3 text-blue-100 no-underline bg-blue-500 hover:bg-blue-600 hover:underline hover:text-blue-200' %>
  <% end %>
</div>
